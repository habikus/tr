            ],
            "ajax": {
                "url": '/ogrenciyukle',
                "type": "GET",
                "datatype": "json",
                "dataSrc": "",
            },
            columns: [{
                    "data": "ogrencino"
                },
                {
                    "data": "ogrenciadi"
                },
                {
                    "data": "sinifi"
                },
            ],
            "language": {
                "url": "//cdn.datatables.net/plug-ins/1.10.12/i18n/Turkish.json"
            }
        });
        // ÖĞRENCİ LİSTESİNİ YÜKLE
    }

    function yeni() {
        temizle();
        $('#form-modal-kaydet').modal('show');
        document.getElementById("modal-baslik-kaydet").innerText = "YENİ KAYIT EKLEME"
        // GÜNÜN TARİHİ        
        var date = new Date();
        var currentDate = date.toISOString().substring(0, 10);
        document.getElementById('islemtarihiK').value = currentDate;
        //+ var tarih_goster=new Date();
        //+ var tarih = tarih_goster.toLocaleString().slice(0, 10); 
        // GÜNÜN TARİHİ

        // İSİM LİSTESİNİ YÜKLE
        /*  $.ajax({
              type: 'GET',
              url: "./ogrenciyukle",
              success: function (data) {
                  data.forEach(function (element) {
                      var option = document.createElement("option");
                      option.text = element.ogrenciadi;
                      ogrenciadiK.add(option);
                  });
              },
          });*/
        // İSİM LİSTESİNİ YÜKLE
    }

    function temizle() {
        //  document.getElementById("aratext").value = "";
        document.getElementById("aciklamaK").value = "";
        document.getElementById("islemtarihiK").value = "";
        document.getElementById("kitapidK").value = "";
        document.getElementById("ogrenciadiK").value = "";
    }

    function izle(data) {
        $('#form-modal-izle').modal('show');
        //  document.getElementById('izle').style.visibility = 'hidden';
        document.getElementById("modal-baslik-izle").innerText = "KAYIT İNCELEME"
        document.getElementById("hareketid").value = data; //linkteki data row yani
        $.ajax({
            type: 'POST',
            url: "./getirhareket",
            data: {
                kelime: data
            },
            success: function (data) {
                // temizle();       
                document.getElementById("hareketid").value = data[0].hareketlerid;
                document.getElementById("aciklama").value = data[0].aciklama;

                var tarih = data[0].islemtarihi.slice(0, 10);
                document.getElementById("islemtarihi").value = tarih;


                //document.getElementById("islemtarihi").value = data[0].islemtarihi;

                document.getElementById("kitapid").value = data[0].kitapadi;
                document.getElementById("ogrenciadi").value = data[0].ogrenciadi;
            },
        });
    }

    function duzenle(data) {
        $('#form-modal-guncelle').modal('show');
        //document.getElementById('guncelle').style.visibility = 'visible';
        document.getElementById("modal-baslik-guncelle").innerText = "KAYIT GÜNCELLEME"
        document.getElementById("hareketidG").value = data; //linkteki data row yani

        $.ajax({
            type: 'POST',
            url: "./getirhareket",
            data: {
                kelime: data
            },
            success: function (data) {
                // temizle();       
                //  document.getElementById("hareketidG").value = data[0].hareketlerid; // yukarda var
                document.getElementById("aciklamaG").value = data[0].aciklama;

                var tarih = data[0].islemtarihi.slice(0, 10);
                document.getElementById('islemtarihiG').value = tarih;

                document.getElementById("kitapidG").value = data[0].kitapadi;
                kId = data[0].kitaplarid;
                document.getElementById("ogrenciadiG").value = data[0].ogrenciadi;
            },
        });
    }

    function silsor(data) {
        var agree = confirm("BU KAYDI SİLMEK İSTEDİĞİNİZDEN EMİN MİSİNİZ?\nBu işlem geri alınamaz!");
        if (agree) {
            //  return true;
            sil(data);
        } else {
            return false;
        }
    }

    function sil(data) {
        $.ajax({
            type: 'POST',
            url: "./silhareket",
            data: {
                kelime: data
            },
            success: function (data) {
                $.notify('Kayıt Silindi', {
                    globalPosition: 'top right',
                    className: 'danger'
                });
                $('#form-modal-kaydet').modal('hide'); // SİL BÖLÜMÜNDE İSE BUNU YAPMADAN DATATABLE REFRESH OLMUYOR
                $('#myTable').DataTable().ajax.reload();
            }
        })
    }

    function guncelle(data) {
        var idtext = document.getElementById("hareketidG").value
        var aciklamatext = document.getElementById("aciklamaG").value;
        var tarihtext = document.getElementById("islemtarihiG").value;
        //var kitapidtext = document.getElementById("kitapidG").value;
        var kitapidtext = kId;
        var ogrenciaditext = document.getElementById("ogrenciadiG").value;
        console.log(kId)
        $.ajax({
            type: 'POST',
            url: "./guncellehareket",
            data: {
                idtxt: idtext,
                aciklamatxt: aciklamatext,
                tarihtxt: tarihtext,
                kitapidtxt: kitapidtext,
                ogrenciaditxt: ogrenciaditext
            },
            success: function (data) {
                $.notify('Kayıt Güncellendi', {
                    globalPosition: 'top right',
                    className: 'success'
                });
                $('#form-modal-guncelle').modal('hide');
                $('#myTable').DataTable().ajax.reload();
            }
        })
    }

    function kaydet() {
        // var idtext = document.getElementById("hareketid").value
        var aciklamatext = document.getElementById("aciklamaK").value;
        var tarihtext = document.getElementById("islemtarihiK").value;
        var kitapidtext = kId; //document.getElementById("kitapidK").value; // NERDEN NE 
        var ogrenciaditext = document.getElementById("ogrenciadiK").value;
        $.ajax({
            type: 'POST',
            url: "./kaydethareket",
