<script>
    var tTable, kTable, oTable;
    var kId;
    //////////////////////////////////
    $(document).ready(function () {
        tTable = $('#myTable').DataTable({
            "dom": 'Blfrtip',
            "buttons": ['copy', 'excel', 'pdf', 'print'],
            "order": [
                [0, "desc"]
            ],
            "ajax": {
                "url": '/hareketyukle',
                "type": "GET",
                dataType: "json",
                "dataSrc": ""
            },
            "columns": [{
                    "data": "hareketlerid"
                },
                {
                    "data": "islemtarihi",
                    'render': function (data, type, full, meta) {
                        if (type === 'display') {
                            data = data.substr(0, 10);
                            var gun = data.substr(8, 2);
                            var ay = data.substr(5, 2);
                            var yil = data.substr(0, 4);
                            data = gun + "." + ay + "." + yil;
                        }
                        return data;
                    },
                    "autoWidth": true,
                    "orderable": true,
                },
                {
                    "data": "kitapadi"
                },
                {
                    "data": "ogrenciadi"
                },
                {
                    "data": "aciklama"
                },
                {
                    "data": "hareketlerid",
                    "render": function (data) {
                        var btnDetay =
                            "<a class='btn btn-info btn-sm text-white' onclick=izle('" +
                            data + "')> <i class='fas fa-info-circle'></i></a>";
                        var btnEdit =
                            "<a class='btn btn-success btn-sm text-white' onclick=duzenle('" +
                            data +
                            "')><i class='fa fa-pencil-square-o' aria-hidden='true'></i></a>";
                        var btnDelete =
                            "<a class='btn btn-danger btn-sm text-white' onclick=silsor('" +
                            data + "')><i class='fa fa-trash'></i></a>";
                        return btnDetay + " | " + btnEdit + " | " + btnDelete;
                    },
                    //  "orderable": false,//
                    "searchable": false,
                    "width": "145px"
                },
            ],
            "language": {
                "url": "//cdn.datatables.net/plug-ins/1.10.12/i18n/Turkish.json"
            }
        });
    });

    function seckitap() {
        if ($.fn.DataTable.isDataTable("#KitapTablo")) {
            $('#KitapTablo').DataTable().clear().destroy();
        }
        $('#KitapListModal').modal('show');
        // KİTAP LİSTESİNİ YÜKLE
        kTable = $("#KitapTablo").DataTable({
            "paging": true,
            "pageLength": 10,
            "responsive": true,
            "autoWidth": false,
            //  "processing": true,
            "order": [
                [1, "asc"]
            ],
            "ajax": {
                "url": '/kitapyukle',
                "type": "GET",
                "datatype": "json",
                "dataSrc": "",
            },
            "columnDefs": [{
                "visible": false,
                "targets": 0
            }],
            columns: [{
                    "data": "kitaplarid"
                },
                {
                    "data": "kitapkodu"
                },
                {
                    "data": "kitapadi"
                },
                {
                    "data": "yazar"
                },
                {
                    "data": "yayinevi"
                }
            ],
            "language": {
                "url": "//cdn.datatables.net/plug-ins/1.10.12/i18n/Turkish.json"
            }
        });
        // KİTAP LİSTESİNİ YÜKLE
    }

    function secogrenci() {
        if ($.fn.DataTable.isDataTable("#OgrenciTablo")) {
            $('#OgrenciTablo').DataTable().clear().destroy();
        }
        $('#OgrenciListModal').modal('show');
        // ÖĞRENCİ LİSTESİNİ YÜKLE
        oTable = $("#OgrenciTablo").DataTable({
            "paging": true,
            "pageLength": 10,
            "responsive": true,
            "autoWidth": false,
            //  "processing": true,
            "order": [
